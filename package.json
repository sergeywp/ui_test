{
  "name": "ui5Demo",
  "version": "0.0.1",
  "description": "UI5 application",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "shx": "^0.3.3",
    "@ui5/cli": "^2.11.2",
    "ui5-middleware-livereload": "^0.5.4",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-ui5": "^2.3.4",
    "npm-run-all": "^4.1.5",
    "eslint": "^7.29.0",
    "ui5-task-nwabap-deployer": "*",
    "ui5-middleware-route-proxy": "*"
  },
  "scripts": {
    "test": "run-s lint karma",
    "start": "ui5 serve --config=uimodule/ui5.yaml  --open index.html",
    "build:ui": "run-s  build:uimodule",
    "karma-ci": "karma start karma-ci.conf.js",
    "clearCoverage": "shx rm -rf coverage",
    "karma": "run-s clearCoverage karma-ci",
    "lint": "eslint .",
    "deploy": "run-s build:ui",
    "serve:uimodule": "ui5 serve --config=uimodule/ui5.yaml",
    "build:uimodule": "ui5 build --config=uimodule/ui5.yaml --clean-dest --dest dist/uimodule"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeywp/ui_test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sergeywp/ui_test/issues"
  },
  "homepage": "https://github.com/sergeywp/ui_test#readme",
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-task-nwabap-deployer",
      "ui5-middleware-route-proxy"
    ]
  }
}
